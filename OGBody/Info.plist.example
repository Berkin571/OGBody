<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>	
	 <!-- Launch-Screen -->
  	<key>UILaunchStoryboardName</key>
  	<string>LaunchScreen</string>

 	 <!-- Bundle-Identity -->
  	<key>CFBundleIdentifier</key>
  	<string>com.koricode.ogbody</string>
  	
	<key>CFBundleName</key>
  	<string>OGBody</string>
  	
	<key>CFBundleExecutable</key>
  	<string>$(EXECUTABLE_NAME)</string>
  	
	<key>CFBundleVersion</key>
  	<string>1</string>
  	
	<key>CFBundleShortVersionString</key>
  	<string>1.0</string>

	<key>NSRemindersUsageDescription</key>
	<string>Die App benötigt Zugriff auf Erinnerungen, um automatische Workout-Erinnerungen anzulegen.</string>

	-
    keys
    -

	<!-- ATS-Ausnahmen für OpenAI -->
  	<key>NSAppTransportSecurity</key>
  	
	<dict>
    	<key>NSAllowsArbitraryLoads</key>
    	<true/>
		<key>NSExceptionDomains</key>
    
	<dict>
      <key>api.openai.com</key>
      <dict>
        <key>----</key>
        <true/>
        <key>----</key>
        <false/>
        <key>----</key>
        <string>TLSv1.2</string>
      </dict>
    </dict>
  </dict>
</dict>
</plist>
